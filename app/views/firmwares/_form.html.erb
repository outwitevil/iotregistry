<%= simple_form_for(@firmware) do |f| %>
  <%= f.error_notification %>

  <div class="form-inputs">
    <%= f.association :iotdevice %>
    <%= f.input :download_url %>
    <%= f.input :version %>
    <%= f.input :name %>
    <%= f.input :readme_url %>
    <%= f.input :readme_title %>
    <%= f.input :readme_description %>
    <%= f.input :enabled %>
    <%= f.input :requirements %>
  </div>

  <div class="form-actions">
    <%= f.button :submit %>
  </div>
<% end %>
